class Solution {
    public int[] sortArrayByParity(int[] A) {
        int n = A.length;
        int start = 0;
        int end = n - 1;
        
        while(start < end){
            if(A[start] % 2 == 1){
                int temp = A[start];
                A[start] = A[end];
                A[end] = temp;
                end--;
            } else{
                start++;
            }
        }
        return A;
    }
}
