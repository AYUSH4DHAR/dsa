/**
 * Definition for a binary tree node.
 * public class TreeNode {
 *     int val;
 *     TreeNode left;
 *     TreeNode right;
 *     TreeNode() {}
 *     TreeNode(int val) { this.val = val; }
 *     TreeNode(int val, TreeNode left, TreeNode right) {
 *         this.val = val;
 *         this.left = left;
 *         this.right = right;
 *     }
 * }
 */
class Solution {
    public void flatten(TreeNode root) {
        if(root==null ) return;
        Stack<TreeNode> s= new Stack<>();
        s.push(root);
        while(!s.isEmpty()){
            TreeNode p = s.pop();
            if(p.right!=null)
                s.push(p.right);
            if(p.left!=null){
                s.push(p.left);
            }
            if(!s.isEmpty())
                p.right=s.peek();
            p.left=null;
        }
    }
}
